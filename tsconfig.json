{
  "compilerOptions": {
    "jsx": "react",
    "module": "commonjs",
    "noImplicitAny": true,
    "outDir": "./build/",
    "preserveConstEnums": true,
    "removeComments": true,
    "sourceMap": true,
    "target": "es5",
    "lib": [
      "es5",
      "es6",
      "dom"
    ],
    "allowJs": true,
    "esModuleInterop": false,
    "moduleResolution": "node"
  },
  "include": [
    "src/components/index.tsx"
  ]
}